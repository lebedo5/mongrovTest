{
  "name": "mongrovTest",
  "displayName": "mongrovTest",
  "expo": {
    "name": "mongrovTest",
    "slug": "mongrovTest",
    "scheme": "mongrovtest",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.mongrovtest",
      "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mongrovtest",
      "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
